	<script
		src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
	<script src="https://developers.kakao.com/sdk/js/kakao.js"></script>
	<script
		src="https://static.nid.naver.com/js/naveridlogin_js_sdk_2.0.2.js"
		charset="utf-8"></script>

	<script>
	    
	        Kakao.init('5ccd00174a49121da7c06055801ba062'); //발급받은 키 중 javascript키를 사용해준다.
	        
	        //카카오로그인
	        function kakaoLogin() {
	            Kakao.Auth.login({
	                success: function (response) {
	                    Kakao.API.request({
	                        url: '/v2/user/me',
	                        success: function (response) {
	                            let info = response.kakao_account; // 로그인 한 사용자 정보
	                            let id = response.id; // 로그인 id
	                            
	                            if (id != null) {
	                                 
	                                 let gender = info.gender;
	                                 
	                                 if (gender.startsWith('m')) {
										gender = 'M';
									} else {
										gender = 'F';
									}
	                                 
	                                 let phone = info.phone_number;
	                                 let cstmrPhone = phone.replace('+82 ','0');
	                                 
	                                 let data = {
	                                	 token: Kakao.Auth.getAccessToken(),
	                                	 userId: id,
	                                	 userName: info.name,
	                                	 userPhone: cstmrPhone,
	                                	 userGender: gender,
	                                	 birthyear: info.birthyear,
	                                	 birthday: info.birthday,
	                                	 email: info.email,
	                                	 socialCtg: "kakao"
	                                 };
	                                 
	                                 let ajax = new XMLHttpRequest();
	                                 ajax.open('POST', '/TasteMap/socialKakao.do', true);
	                                 ajax.setRequestHeader('Content-Type', 'application/json');
	                                 
	                                 ajax.onload = function () {
	                                	 
										if (ajax.status == 200) {
											
											if (ajax.responseText == "ok") {
																						
												location.href = "/TasteMap/login.do?userId=" + id + "&socialCtg=" + data.socialCtg;
														
											} else {
												
												location.href = "/TasteMap/views/customer/registration.html";
											}
										}
									}
	                                 ajax.send(JSON.stringify(data));
								}
	                        },
	                        fail: function (error) {
	                            console.log(error)
	                        },
	                    })
	                },
	                fail: function (error) {
	                    console.log(error)
	                },
	            })
	        }
	        
	        
	        let naverLogin = new naver.LoginWithNaverId(
	                {
	                    clientId: "UHX4_b3xDvYEMiiamTbB", //내 애플리케이션 정보에 cliendId를 입력해줍니다.
	                    callbackUrl: "http://localhost:8888/TasteMap/views/customer/login.html", // callback url jsp로 지정해서 해보기
	                    isPopup: false,
	                    callbackHandle: true
	                }
	            );
	        
	        naverLogin.init();
	        
	        document.querySelector('#naverIdLogin_loginButton').addEventListener('click', function () {
	        	
	            naverLogin.getLoginStatus(function (status) {
	            	
	                if (status) {
	                	
	                	let info = naverLogin.user;
	                    
	                    let ajax = new XMLHttpRequest();
                        ajax.open('POST', '/TasteMap/socialNaver.do', true);
                        ajax.setRequestHeader('Content-Type', 'application/json');
                        
                        let data = {
                           	 token: naverLogin.accessToken.accessToken,
                        	 userId: info.getId(),
                        	 userName: info.getName(),
                        	 userPhone: info.getMobile(),
                        	 userGender: info.getGender(),
                        	 birthyear: info.getBirthyear(),
                        	 birthday: info.getBirthday(),
                        	 email: info.getEmail(),
                        	 socialCtg: "naver"
                         };
                        
                        ajax.onload = function () {
                       	 
							if (ajax.status == 200) {
								if (ajax.responseText == "ok") {
									
									location.href = "/TasteMap/login.do?userId=" + info.getId() + "&socialCtg=naver"; 
											
								} else {
									alert('등록되지 않은 계정입니다.');
									location.replace("/TasteMap/views/customer/registration.html");
								}
							}
						}
                        ajax.send(JSON.stringify(data));
	                   

	                } else {
	                    console.log(status);
	                }
	            });
	        });
	    
	           
	        
	        
	
</script>
