	<script
		src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
	<script>
		let mailCheckPw = false;
		
		let key;
		
		$('#sendMail').click(function () { // 인증 이메일 전송
			
			let email =  $('input[name="cstmrEmail"]').val();
		
			if (email == null) {
				alert('이메일을 입력해주세요.');
			} else {
				alert("인증번호를 발송했습니다.");
				$.ajax({
					method : 'post',
					url : '/TasteMap/mail.do',
					data : {
					cstmrEmail: email
					},
					success : function(rst) {
						if (rst != null) { 
							key = rst;
						} else {
							alert("인증번호 발송에 실패했습니다.");
						}
					},
					error : function(xhr) {
						alert(xhr.status);
					}
			});

		}

	});

		$('#mailChk').click(function() { // 인증번호 일치 여부 확인
			// 인증번호 일치여부 확인 후 입력 readonly로 변경
			let ketVal = $('#no').val();

			if (ketVal == key) {
				alert('인증에 성공하였습니다.');
				$('input[name="cstmrEmail"]').prop('readOnly', true);
				$('#no').prop('readOnly', true);
				mailCheckPw = true;
			} else {
				alert('인증에 실패하였습니다!');
				mailCheckPw = false;
			}

		});
		
		function valid() {
			
			event.preventDefault();

			let cstmrId =  $('#cstmrId').val();
			let cstmrEmail =  $('#cstmrEmail').val();
			
			$.ajax({
				method : 'post',
				url : 'getCstmrId.jsp',
				data : {
					cstmrId: cstmrId,
					cstmrEmail: cstmrEmail
				},
				success : function(id) {
					if (id != null) { 
						$('#findPw').submit();
					}
				},
				error : function(xhr) {
					alert(xhr.status);
					}
				});
			
	}
		
		
		
		
		
	</script>
