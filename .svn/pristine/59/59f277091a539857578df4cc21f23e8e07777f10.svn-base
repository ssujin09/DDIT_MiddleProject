    <section class="login registration section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 offset-lg-3 col-md-8 offset-md-2 col-12">
                    <div class="form-head">
                        <h4 class="title">회원가입</h4>
                        <form id="registForm" method="post" onsubmit="valid()">
                            <div class="socila-login">
                                <ul>
                                    <li><a href="javascript:void(0)" class="facebook"><i class="lni lni-facebook-original"></i>네이버로 시작하기</a></li>
                                    <li><a href="javascript:void(0)" class="google"><i class="lni lni-google"></i>카카오로 시작하기</a>
                                    </li>
                                </ul>
                            </div>
                            <div class="alt-option">
                                <span>또는</span>
                            </div>
                            <div class="form-group">
                                <label>아이디</label>
                                <input name="cstmrId" type="text" required pattern="[a-zA-Z][a-zA-Z0-9]{3,7}">
                            </div>
                            <div class="form-group">
                                <label>비밀번호</label>
                                <input name="cstmrPw" type="password" required pattern="(?=.*?[a-z])(?=.*?[A-Z])(?=.*?[0-9])(?=.*?[!@#\$%\^&amp;\*\(\)_\+\|]).{8,}">
                            </div>
                            <div class="form-group">
                                <label>비밀번호 확인</label>
                                <input type="password" required pattern="(?=.*?[a-z])(?=.*?[A-Z])(?=.*?[0-9])(?=.*?[!@#\$%\^&amp;\*\(\)_\+\|]).{8,}">
                            </div>
                            <div class="form-group">
                                <label>이름</label>
                                <input name="cstmrName" type="text" required pattern="[가-힣]{2,8}">
                            </div>
                            <div class="form-group">
                                <label>생년월일</label>
                                <input name="cstmrBir" type="date">
                            </div>
                            <div class="form-group">
                                <label>전화번호</label>
                                <input name="cstmrPhone" type="text">
                            </div>
                            <!-- 성별 체크박스 추가 -->
                            <div class="form-group">
                                <label>이메일</label>
                                <input name="cstmrEmail" type="email" pattern="^[0-9a-zA-Z]+@[0-9a-zA-Z]+(\.[a-z]+){1,2}">
                            </div>
                            <div class="form-group">
                                <label>주소</label>
                                <input name="cstmrAddr" type="text">
                            </div>
                            <div class="form-group">
                                <label>닉네임</label>
                                <input name="cstmrNick" type="text">
                            </div>
                            <div class="check-and-pass">
                                <div class="row align-items-center">
                                    <div class="col-12">
                                         <span id="joinspan"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="button">
                                <button type="submit" class="btn">Registration</button>
                            </div>
                            <p class="outer-link">이미 계정이 있으신가요? <a href="login.html"> 로그인</a>
                            </p>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
