    <script>
    	
    function valid() {
		
		  // submit의 기본기능을 막아서action(현재 url)의 위치로 이동 중지
		  event.preventDefault(); 
		  
		  // form안의 입력양식 요소들의 정보를 직렬화(jQuery serialize())하고 서버페이지로 전달예정
		  let data = $('#registForm').serialize();
		  
		  // serializeArray(): name과 value로 구성된 연관배열 형태의 값을 생성해줌
		  // JSON.stringify(form.serializeArray())한 결과를 서버페이지로 전송하고 처리하기는 개인적으로 해보기
		  
		  // 회원정보 insert
		  $.ajax({
			method:'post',
			url: 'serialize.jsp',
			data: data,
			success: function (rst) {
				if (rst == 1) {
				let conf = confirm("회원가입에 성공하셨습니다. 로그인 페이지로 이동하시겠습니까?");
				if (conf) location.href="login.html";
					$('#joinspan').text("가입성공").css('color', 'limegreen');
					$(':input').val(""); // 가입 성공시 작성된 내용 초기화
				}else {
					$('#joinspan').text("가입실패").css('color', 'orangered');
				}
			},
			error: function (xhr) {
				alert(xhr.status);
			}
		  });
		  
		  
		  }
    
    
    	
    </script>
