    <script>

        let idCheck = false;
        let mailCheck = false;
        let passCheck = false;
        let nickCheck = false;

        $('#idChk').on('click', function name() {

            let id = $('#idVal').val();
            let pattern = /^[a-z]+[a-z0-9]{5,15}$/g;
            if (id != null && pattern.test(id)) {
                $.ajax({
                    url: 'idCheck.jsp',
                    data: {
                        id: id
                    },
                    success: function (rst) {
                        if ($('#idVal').val() == null) {
                            $('#disp').text('');
                        } else {
                            if (rst.rst == "ok") {
                            	
                                $('#disp').text(rst.msg).css('color', 'limegreen');
                                $('#idVal').prop('readOnly', true);
                                $('#idChk').prop('disabled', true);
                                idCheck = true;
                                
                            } else {
                            	
                                $('#disp').text(rst.msg).css('color', 'orangered');
                            }
                        }

                    },
                    error: function (xhr) {
                        alert(xhr.status)
                    },
                    dataType: 'json'
                });
            } else {
                alert('ÏïÑÏù¥ÎîîÍ∞Ä ÌòïÏãùÍ≥º ÏùºÏπòÌïòÏßÄ ÏïäÏäµÎãàÎã§! Îã§Ïãú ÌïúÎ≤à ÌôïÏù∏Ìï¥ Ï£ºÏÑ∏Ïöî');
            }


        });

        function valid() {

            event.preventDefault();

            let data = new FormData($('#registForm')[0]);

            $.ajax({
                method: 'post',
                url: '/TasteMap/regist.do',
                data: data,
                processData: false,
