                success: function (rst) {
                	
                	let result = rst.trim();
                	
                	let socialCtg = document.querySelector('#social');
                	if (mailCheck && socialCtg.value != null) { // ÏÜåÏÖúÌÖåÍ∑∏ ÏûàÏúºÎ©¥ ÏßÑÌñâ
							socialJoin();
                        	let conf = confirm("ÌöåÏõêÍ∞ÄÏûÖÏóê ÏÑ±Í≥µÌïòÏÖ®ÏäµÎãàÎã§. Î°úÍ∑∏Ïù∏ ÌéòÏù¥ÏßÄÎ°ú Ïù¥ÎèôÌïòÏãúÍ≤†ÏäµÎãàÍπå?");
                        	if (conf)
                            	location.href = "login.html";
                        	$('#joinspan').text("Í∞ÄÏûÖÏÑ±Í≥µ").css('color', 'limegreen');
                        	$(':input').val(""); // Í∞ÄÏûÖ ÏÑ±Í≥µÏãú ÏûëÏÑ±Îêú ÎÇ¥Ïö© Ï¥àÍ∏∞Ìôî
					} else {
                    	if (result == "ok" && (idCheck && mailCheck && passCheck && nickCheck)) { // ÌöåÏõêÍ∞ÄÏûÖ Ï°∞Í±¥ Ï∂îÍ∞Ä (idCkeck, mailCheck == 1)
                        	let conf = confirm("ÌöåÏõêÍ∞ÄÏûÖÏóê ÏÑ±Í≥µÌïòÏÖ®ÏäµÎãàÎã§. Î°úÍ∑∏Ïù∏ ÌéòÏù¥ÏßÄÎ°ú Ïù¥ÎèôÌïòÏãúÍ≤†ÏäµÎãàÍπå?");
                        	if (conf) location.href = "login.html";
                        	$('#joinspan').text("Í∞ÄÏûÖÏÑ±Í≥µ").css('color', 'limegreen');
                        	$(':input').val(""); // Í∞ÄÏûÖ ÏÑ±Í≥µÏãú ÏûëÏÑ±Îêú ÎÇ¥Ïö© Ï¥àÍ∏∞Ìôî
                    	} else {
                        	$('#joinspan').text("Í∞ÄÏûÖÏã§Ìå®").css('color', 'orangered');
                    	}
					}

                },
