	<script>
		let idCheck = false;
		let mailCheck = false;
		let passCheck = false;
		let nickCheck = false;

		$('#idChk').on(
				'click',
				function name() {

					let id = $('#idVal').val();
					let pattern = /^[a-z]+[a-z0-9]{5,15}$/g;
					if (id != null && pattern.test(id)) {
						$.ajax({
							url : 'idCheck.jsp',
							data : {
								id : id
							},
							success : function(rst) {
								if ($('#idVal').val() == null) {
									$('#disp').text('');
								} else {
									if (rst.rst == "ok") {

										$('#disp').text(rst.msg).css('color',
												'limegreen');
										$('#idVal').prop('readOnly', true);
										$('#idChk').prop('disabled', true);
										idCheck = true;

									} else {

										$('#disp').text(rst.msg).css('color',
												'orangered');
									}
								}

							},
							error : function(xhr) {
								alert(xhr.status)
							},
							dataType : 'json'
						});
					} else {
						alert('아이디가 형식과 일치하지 않습니다! 다시 한번 확인해 주세요');
					}

				});
		
		document.getElementById('year').addEventListener('change', updateDateInput);
		document.getElementById('month').addEventListener('change', updateDateInput);
		document.getElementById('day').addEventListener('change', updateDateInput);

		// 이벤트가 발생할 때 호출될 함수
		function updateDateInput() {
		    // 각 select 요소의 선택된 값을 가져옴
		    const selectedYear = document.getElementById('year').value;
		    const selectedMonth = document.getElementById('month').value;
		    const selectedDay = document.getElementById('day').value;

		    // 만약 연도, 월, 일이 모두 선택되었다면
		    if (selectedYear !== '년' && selectedMonth !== '월' && selectedDay !== '일') {
		        // 입력 요소에 YYYY-MM-DD 형식으로 값을 설정
		        document.getElementById('cstmrBir').value = `${selectedYear}-${selectedMonth}-${selectedDay}`;
		    } else {
		        // 하나라도 선택되지 않았다면 입력 요소를 비움
		        document.getElementById('cstmrBir').value = '';
		    }
		}
		
		document.getElementById('gender').addEventListener('change', updateGenderInput);
		
		function updateGenderInput() {
		    // 각 select 요소의 선택된 값을 가져옴
		    const selectedGender = document.getElementById('gender').value;

		    // 만약 연도, 월, 일이 모두 선택되었다면
		    if (selectedGender !== '선택하세요') {
		        // 입력 요소에 YYYY-MM-DD 형식으로 값을 설정
		        document.getElementById('cstmrGender').value = `${selectedGender}`;
		    } else {
		        // 하나라도 선택되지 않았다면 입력 요소를 비움
		        document.getElementById('cstmrGender').value = '';
		    }
		}

		function valid() {

			event.preventDefault();

			let data = new FormData($('#registForm')[0]);
			
			for (const [key, value] of data.entries()) {
				  console.log(`${key}: ${value}`);
				}

			$.ajax({
						method : 'post',
						url : '/TasteMap/regist.do',
						data : data,
						processData : false,
						contentType : false,
						success : function(rst) {

							let result = rst.trim();

							let socialCtg = document.querySelector('#social');
							if (mailCheck && socialCtg.value != null) { // 소셜테그 있으면 진행
								socialJoin();
								let conf = confirm("회원가입에 성공하셨습니다. 로그인 페이지로 이동하시겠습니까?");
								if (conf)
									location.href = "login.html";
								$('#joinspan').text("가입성공").css('color',
										'limegreen');
								$(':input').val(""); // 가입 성공시 작성된 내용 초기화
							} else {
								if (result == "ok"
										&& (idCheck && mailCheck && passCheck && nickCheck)) { // 회원가입 조건 추가 (idCkeck, mailCheck == 1)
									let conf = confirm("회원가입에 성공하셨습니다. 로그인 페이지로 이동하시겠습니까?");
									if (conf)
										location.href = "login.html";
									$('#joinspan').text("가입성공").css('color',
											'limegreen');
									$(':input').val(""); // 가입 성공시 작성된 내용 초기화
								} else {
									$('#joinspan').text("가입실패").css('color',
											'orangered');
								}
							}

						},
						error : function(xhr) {
							alert(xhr.status);
						}
					});

		}

		$('#passChk').keyup(
				function() {
					let cstmrPass = $('#cstmrPw').val();
					let passChk = $('#passChk').val();

					if (cstmrPass == passChk) {
						passCheck = true;
						$('#dispPw').text("");
					} else {
						passCheck = false;
						$('#dispPw').text("비밀번호가 일치하지 않습니다. 다시 한번 확인해주세요.")
								.css('color', 'orangered');
					}
				});

		const fileDOM = document.querySelector('#profileImg');
		const preview = document.querySelector('.image-box');
		$('#profileImg').on('change', function () {
			  const imageSrc = URL.createObjectURL(fileDOM.files[0]);
			  preview.src = imageSrc;
		});

		function fileChk(file) {

			if (!/\.(jpeg|jpg|png|gif|bmp)$/i.test(file.value)) {

				alert('이미지 파일만 업로드 가능합니다.');

				file.value = '';

				file.focus();

			}
		}

		$('#addrBtn').click(f_addr);

		function f_addr() {
			new daum.Postcode({
				oncomplete : function(data) {
					// 팝업에서 검색결과 항목을 클릭했을때 실행할 코드를 작성하는 부분.

					// 도로명 주소의 노출 규칙에 따라 주소를 표시한다.
					// 내려오는 변수가 값이 없는 경우엔 공백('')값을 가지므로, 이를 참고하여 분기 한다.
					var roadAddr = data.roadAddress; // 도로명 주소 변수
					var extraRoadAddr = ''; // 참고 항목 변수

					// 법정동명이 있을 경우 추가한다. (법정리는 제외)
					// 법정동의 경우 마지막 문자가 "동/로/가"로 끝난다.
					if (data.bname !== '' && /[동|로|가]$/g.test(data.bname)) {
						extraRoadAddr += data.bname;
					}
					// 건물명이 있고, 공동주택일 경우 추가한다.
					if (data.buildingName !== '' && data.apartment === 'Y') {
						extraRoadAddr += (extraRoadAddr !== '' ? ', '
								+ data.buildingName : data.buildingName);
					}
					// 표시할 참고항목이 있을 경우, 괄호까지 추가한 최종 문자열을 만든다.
					if (extraRoadAddr !== '') {
						extraRoadAddr = ' (' + extraRoadAddr + ')';
					}

					// 우편번호와 주소 정보를 해당 필드에 넣는다.
					document.getElementById("addr1").value = roadAddr;
					/* document.getElementById("sample4_jibunAddress").value = data.jibunAddress; */

				}
			}).open();
		}

		let key;

		$('#sendMail').click(function() { // 인증 이메일 전송

			let email = $('input[name="cstmrEmail"]').val();

			if (email != null && email != "") {
				alert("인증번호를 발송했습니다.");
				$.ajax({
					method : 'post',
					url : '/TasteMap/mail.do',
					data : {
						cstmrEmail : email
					},
					success : function(rst) {
						if (rst != null) {
							key = rst;
						} else {
							alert("인증번호 발송에 실패했습니다.");
						}
					},
					error : function(xhr) {
						alert(xhr.status);
					}
				});
			} else {
