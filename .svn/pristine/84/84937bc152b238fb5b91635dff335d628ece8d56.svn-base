	<script
		src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
	<script>
		let mailCheckId = false;
		
		let key;
		
		$('#sendMail').click(function () { // 인증 이메일 전송
			
			let email =  $('input[name="cstmrEmail"]').val();
		
			if (email == null) {
				alert('이메일을 입력해주세요.');
			} else {
				
			$.ajax({
				method : 'post',
				url : '/TasteMap/mail.do',
				data : {
					cstmrEmail: email
				},
				success : function(rst) {
					if (rst != null) { 
						alert("인증번호를 발송했습니다.");
						key = rst;
					} else {
						alert("인증번호 발송에 실패했습니다.");
					}
				},
				error : function(xhr) {
					alert(xhr.status);
				}
			});

		}

	});

		$('#mailChk').click(function() { // 인증번호 일치 여부 확인
			// 인증번호 일치여부 확인 후 입력 readonly로 변경
			let ketVal = $('#no').val();

			if (ketVal == key) {
				alert('인증에 성공하였습니다.');
				$('input[name="cstmrEmail"]').prop('readOnly', true);
				$('#no').prop('readOnly', true);
				mailCheckId = true;
			} else {
				alert('인증에 실패하였습니다. 이름과 이메일을 다시 한번 확인해 주세요.');
				mailCheckId = false;
			}

		});
		
		
		$('#searchId').click(function () {
			
			if (mailCheckId) {
				$.ajax({
					url : 'searchId.jsp',
					data : {
						cstmrName : $('#cstmrName').val(),
						cstmrEmail : $('#cstmrEmail').val()	
					},
					success : function(rst) {
						
						if (rst != null) { // 소셜 계졍인 경우 아이디 대신 소셜 정보 알려주기
							if (rst.trim() == 'kakao' || rst.trim() == 'naver') {
								$('#disp').text(rst); // 문구 수정하기
							} else {
								$('#disp').text("아이디 : " + rst);
							}
						} else {
							alert("가입하신 아이디가 존재하지 않습니다. 다시 한번 확인해 주세요.");
						}
					},
					error : function(xhr) {
						alert(xhr.status);
					}
				});
			} else {
				alert('이메일 인증이 완료되지 않았습니다.');
			}
			
		});
		
		
	</script>
